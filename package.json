{
  "name": "cedula",
  "displayName": "Cedula",
  "version": "0.0.1",
  "description": "A Filipino community verification plugin for Facebook",
  "author": "Gil Emmanuel Bancud",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@babel/core": "^7.19.1",
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.4.0",
    "@mantine/hooks": "^5.4.0",
    "@plasmohq/storage": "^0.11.1",
    "@tabler/icons": "^1.97.0",
    "axios": "^0.27.2",
    "firebase": "^9.9.4",
    "plasmo": "0.53.0",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "1.2.0",
    "@types/chrome": "0.0.196",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "manifest": {
    "oauth2": {
      "client_id": "$PLASMO_PUBLIC_FIREBASE_CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }
}
